<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.com.zh.dragcontentlayout">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BottomSheetDemoActivity" />
        <activity android:name=".ui.SpannableStringActivity" />
        <activity android:name=".ui.SelectWordsFillBlanksActivity" />
        <activity android:name=".ui.SelectWordsFillBlanksActivity2" />
        <activity android:name=".ui.SelectWordsFillBlanksActivity3" />
        <activity android:name=".ui.SelectWordsFillBlanksMultipleActivity" />
        <activity android:name=".ui.VoiceTestActivity" />
    </application>

</manifest>